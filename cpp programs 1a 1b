//To print size of datatypes
#include <iostream>
using namespace std;
int main(){
	cout<<"Size of char:  "<<sizeof(char)<<" byte\n";
	cout<<"Size of int: "<<sizeof(int)<<" byte\n";
	cout<<"Size of float: "<<sizeof(float)<<" byte\n";
	cout<<"Size of double: "<<sizeof(double)<<" byte\n";
	
	return 0;
	
}


//Prints the product of two numbers
#include <iostream>
using namespace std;

void main(){
	double a,b;
	cout<<"Enter two numbers: ";
	cin>>a>>b;
	cout<<"Product= "<<a*b<<endl;
	
	 
}
